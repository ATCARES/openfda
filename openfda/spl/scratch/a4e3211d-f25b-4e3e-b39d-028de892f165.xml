               <code codeSystem="2.16.840.1.113883.6.1" code="34089-3" displayName="DESCRIPTION SECTION"/>
               <title>
                  <content styleCode="bold italics">Purpose</content>
               </title>
               <text>
                  <paragraph>Sunscreen</paragraph>
               </text>
               <effectiveTime value="20100327"/>
